<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mini Whatsapp</title>
</head>
<body>
<h3>All Chats!</h3>
<% chats.forEach(function(chat) { %>
  <div>
    <p>From: <%= chat.from %></p>
    <p><%= chat.msg %></p>
    <p>Received by: <%= chat.to %></p>
    <hr/>
  </div>
<% }); %>


    
</body>
</html>
